<%= form_for(@user, html: {multipart: true}) do |f| %>
  <ul>
  <% @user.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
  <p>
    <%= f.label :roll_no %><br />
    <%= f.text_field :roll_no %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_area :name %>
  </p>
  

  <p>

    <%= f.submit %>
  </p>
 

<% end %> 
